{
  "expo": {
    "name": "Vouch",
    "slug": "Vouch",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": ["vouch"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.solo.dev.Vouch"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "root": "./screens"
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFF",
          "image": "./assets/images/splash-icon.png",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#FFF"
          },
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
   
    "extra": {
      "router": {
        "origin": false,
        "root": "./screens"
      },
      "eas": {
        "projectId": "d385756d-3ad7-4a65-a473-3faf1bf45aac"
      }
    }
  }
}
